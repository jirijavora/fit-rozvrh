{
  "name": "fit-rozvrh",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "lint:fix": "eslint --fix src",
    "prettier": "prettier --check src",
    "prettier:fix": "prettier --write src",
    "format": "pnpm run lint:fix && pnpm run prettier:fix",
    "tsc:check": "tsc --noEmit --incremental",
    "precommit": "pnpm tsc:check && pnpm prettier && pnpm lint"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@vercel/kv": "^2.0.0",
    "bootstrap": "^5.3.3",
    "classnames": "^2.5.1",
    "date-fns": "^2.29.3",
    "date-fns-tz": "^2.0.0",
    "node-fetch": "^3.3.0",
    "oauth4webapi": "^2.16.0",
    "react": "^18.3.1",
    "react-bootstrap": "^2.10.4",
    "react-dom": "^18.3.1",
    "react-icons": "^4.3.1",
    "sass": "^1.78.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-react": "^7.36.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.5.0",
    "vite": "^5.4.4",
    "vite-plugin-pwa": "^0.20.5"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
